<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.tdaderp" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#FFD500</item>
        <item name="colorPrimaryVariant">#00FF04</item>
        <item name="colorOnPrimary">#002AFF</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#FFCA5E</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#F1F6FF</item>
        <item name="android:navigationBarColor">@color/black</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="CustomRatingBarStyle" parent="Widget.AppCompat.RatingBar">
        <item name="android:progressDrawable">@drawable/rating</item>
        <item name="android:minHeight">15dip</item>
        <item name="android:maxHeight">47dip</item>
    </style>
</resources>